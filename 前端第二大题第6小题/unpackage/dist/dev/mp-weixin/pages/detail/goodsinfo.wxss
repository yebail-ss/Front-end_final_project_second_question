	-webkit-transition: all .4s ease 0s;
	-webkit-transform-origin: center;
	    -ms-transform-origin: center;
	        transform-origin: center;
}
.scroll_top.active {
	opacity: 1;
	-webkit-transform: translateY(-25px) translateX(0);
